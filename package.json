{
  "name": "@danbruegge/gatsby-starter-kitt",
  "description": "Gatsby starter kitt with typescript, mdx, linting and styled-components",
  "version": "1.0.0",
  "author": "Daniel Br√ºggemann <hello@danbruegge.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby serve",
    "dev": "NODE_ENV=dev gatsby develop --port 3000 --host 0.0.0.0",
    "prebuild": "yarn lint:types && yarn lint:es && yarn lint:style",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "format": "prettier --ignore-path .gitignore --write '**/*.{js,jsx,ts,tsx,json}'",
    "lint:es": "eslint --ignore-path .gitignore --fix --ext .js,.jsx,.ts,.tsx .",
    "lint:types": "tsc --noEmit",
    "lint:style": "stylelint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "yarn lint:es && yarn lint:types && yarn lint:style",
    "test": "jest --config jest/config.js src",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^2.28.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-manifest": "^2.8.0",
    "gatsby-plugin-mdx": "^1.6.0",
    "gatsby-plugin-offline": "^3.6.0",
    "gatsby-plugin-react-helmet": "^3.6.0",
    "gatsby-plugin-styled-components": "^3.6.0",
    "gatsby-plugin-typegen": "^2.2.1",
    "gatsby-source-filesystem": "^2.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@danbruegge/gatsby-plugin-stylelint": "^4.1.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.4",
    "@types/styled-system": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.8.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-dom": "^3.6.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.1.2"
  },
  "keywords": [
    "gatsby",
    "starter",
    "mdx",
    "typescript",
    "eslint",
    "prettier",
    "stylelint",
    "styled-components",
    "styled-system",
    "jest"
  ]
}
